{
  "name": "forecaster",
  "version": "1.0.0",
  "description": "Forecaster using Dark Sky API",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha './app/**/*.spec.js' --compilers js:babel-core/register --reporter list",
    "test:watch": "npm test -- --watch",
    "cover": "nyc --reporter=html --reporter=text npm test",
    "start": "webpack-dev-server --inline --hot --config webpack.dev.config.js --content-base dist --history-api-fallback",
    "prod": "npm run build:client && npm run build:server && node server.bundle.js",
    "build:client": "webpack --config webpack.client.config.js",
    "build:server": "webpack --config webpack.server.config.js"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "app/Forecast/Tests"
    ]
  },
  "author": "Shawn Moore",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-form": "^6.0.5",
    "redux-thunk": "^2.1.0",
    "skycons": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.17.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "mocha": "^3.1.0",
    "node-sass": "^3.10.1",
    "nyc": "^8.3.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.7.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
